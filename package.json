{
  "name": "leaflet.control.nestedlayers",
  "version": "0.1.0",
  "description": "A variant of the built-in L.control.layers that accepts layers in a hierarchy",
  "author": {
    "name": "Colby Rogness",
    "url": "https://github.com/colbin8r",
    "twitter": "colbyrogness"
  },
  "license": "MIT",
  "homepage": "https://bitbucket.org/worldviewsolutions/leaflet.control.nestedlayers#readme",
  "repository": {
    "type": "git",
    "url": "git+https://colbin8r@bitbucket.org/worldviewsolutions/leaflet.control.nestedlayers.git"
  },
  "keywords": [
    "leaflet",
    "gis",
    "layer",
    "map"
  ],
  "main": "/dist/index.js",
  "scripts": {
    "build": "gulp build",
    "pack": "webpack --config webpack.config.js",
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "lint": "eslint src",
    "lint:test": "eslint test",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "test:integration": "npm run lint && mocha './test/integration/**/*.spec.js' --compilers js:babel-core/register --require './test/mocha.conf.js'",
    "test:unit": "mocha './test/unit/**/*.spec.js' --compilers js:babel-core/register --require './test/mocha.conf.js'",
    "test-watch": "mocha './test/unit/**/*.spec.js' --compilers js:babel-core/register --require './test/mocha.conf.js' --watch",
    "start": "gulp watch"
  },
  "dependencies": {
    "leaflet-headless": "^0.2.5",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "color-logs": "^0.6.1",
    "colors": "^1.1.2",
    "del": "^2.2.2",
    "eslint": "^4.0.0",
    "eslint-config-idiomatic": "^3.1.0",
    "eslint-plugin-mocha": "^4.0.0",
    "eslint-plugin-react": "^7.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-format-md": "*",
    "gulp-sourcemaps": "^1.6.0",
    "jsdom": "^11.0.0",
    "mocha": "*",
    "nyc": "^7.0.0",
    "path": "^0.12.7",
    "sinon": "^2.3.5",
    "webpack": "^3.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
